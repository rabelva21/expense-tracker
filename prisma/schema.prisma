generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id          String   @id @default(uuid())
  text        String
  amount      Float
  category    TransactionCategory 
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

enum TransactionCategory {
  INCOME
  EXPENSE
}