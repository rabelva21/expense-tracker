// prisma/schema.prisma

generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
}

model Transaction {
  id          String   @id @default(uuid())
  text        String
  amount      Float
  type        TransactionType // Income atau Expense
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

enum TransactionType {
  INCOME
  EXPENSE
}